
@{
    Layout = null;
}

@*@model DataLayer.ViewModel.ChangePasswordInNotSignInViewModel*@

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>تایید شماره تلفن</title>
    <link href="~/UIKit/css/uikit.min.css" rel="stylesheet" />
    <link href="~/StyleCenter/Sass/Controls.min.css" rel="stylesheet" />
    <link href="~/StyleCenter/Sass/LoginRegister/loginRegister.min.css" rel="stylesheet" />
</head>
<body>
    <div class="RegisterContainer">
        <div class="firstHead"></div>
        <div class="LoginContent my-clipped mat ">
            <div class="center my-column Forget">
                <form class="my-column uk-padding-small" dir="rtl">
                    <h3 class="uk-margin-remove-bottom uk-margin-top">کد فعالسازی را وارد کنید</h3>
                    <div class="uk-margin">
                        <input type="hidden" name="TellNo" />
                        <input name="ActiveCode" class="uk-input" type="text">
                    </div>
                    <h3 class="uk-margin-remove">رمز عبور جدید را وارد کنید</h3>
                    <div class="uk-margin">
                        <input type="hidden" name="TellNo" />
                        <input name="ActiveCode" class="uk-input" type="text">
                    </div>
                    <h3 class="uk-margin-remove">تکرار رمز عبور</h3>
                    <div class="uk-margin">
                        <input type="hidden" name="TellNo" />
                        <input name="ActiveCode" class="uk-input" type="text">
                    </div>
                    <button class="gradientButton center uk-text-center" href="#success" uk-toggle>
                        ارسال
                    </button>
                </form>
            </div>
        </div>
    </div>
    <div id="success" uk-modal>
    </div>
</body>
</html>
<script src="~/UIKit/js/uikit.min.js"></script>
<script src="~/Jquery/jquery-3.4.1.js"></script>
